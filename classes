from student import Student

student1 = Student("Matt", "Central Banking", 3.5, False)
student2 = Student("Phil", "History", 2.1, True)
print(student2.major)


#this is another file called Student
class Student:
    def __init__(self, name, major, gpa, is_on_probation):
        self.name = name
        self.major = major
        self.gpa = gpa
        self.is_on_probation = is_on_probation
